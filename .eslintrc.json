{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "ignorePatterns": ["node_modules/", ".next/", "out/"],
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/no-unknown-property": "off",
    "react/no-unused-vars": "off",
    "react/no-deprecated": "off"
  }
}